{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pracdatafactory110"
		},
		"Ls_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ls_ADLS'"
		},
		"Ls_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspracstorage110.dfs.core.windows.net/"
		},
		"Ls_HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/harsha04v/"
		},
		"ls_JsonLookup_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/harsha04v/Lego-Azure-pipeline/refs/heads/main/foreach.json"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pracdatafactory110_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_ADLS_accountKey": {
						"value": "[parameters('Ls_ADLS_accountKey')]"
					},
					"Ls_ADLS_properties_typeProperties_url": {
						"value": "[parameters('Ls_ADLS_properties_typeProperties_url')]"
					},
					"Ls_HTTP_properties_typeProperties_url": {
						"value": "[parameters('Ls_HTTP_properties_typeProperties_url')]"
					},
					"ls_JsonLookup_properties_typeProperties_url": {
						"value": "[parameters('ls_JsonLookup_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}